{
  "name": "dataset-config",
  "version": "1.2.0",
  "description": "Parse HTML data attributes into a structured object with automatic type conversion.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajiho/dataset-config.git"
  },
  "bugs": {
    "url": "https://github.com/ajiho/dataset-config/issues"
  },
  "author": "ajiho",
  "funding": "https://www.lujiahao.com/sponsor",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky || true",
    "prettier": "prettier --config configs/prettier.config.mjs . --check --cache --cache-location node_modules/.cache/.prettiercache",
    "prettier:fix": "prettier --config configs/prettier.config.mjs . --write",
    "eslint": "eslint -c configs/eslint.config.mjs --cache --cache-location node_modules/.cache/.eslintcache",
    "eslint:fix": "eslint -c configs/eslint.config.mjs --fix",
    "commitlint": "commitlint --config configs/commitlint.config.mjs -e -V",
    "size": "size-limit --config configs/size-limit.mjs",
    "lint": "run-s prettier eslint",
    "fix": "run-s *:fix",
    "build:base": "tsdown --config configs/tsdown.config.mjs",
    "build:min": "cross-env NODE_ENV=production run-s build:base",
    "build": "run-s build:* size",
    "test": "vitest --config configs/vitest.config.mjs --coverage --ui",
    "coverage": "vitest --config configs/vitest.config.mjs run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "attribute-parser",
    "camelcase",
    "data attribute",
    "data attributes",
    "data attrs",
    "data-conversion",
    "data-extraction",
    "data-parser",
    "data-to-object",
    "data-utils",
    "dataset",
    "deep-object",
    "dom-data",
    "exclude-keys",
    "HTML element",
    "html-data",
    "prefix-filter",
    "type-parsing"
  ],
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "conventional-changelog-angular": "^8.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "execa": "^9.6.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "npm-run-all2": "^8.0.4",
    "picocolors": "^1.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-sort-pkgjson": "^1.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.7.3",
    "size-limit": "^11.2.0",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.7"
  }
}
